{
    "name": "vscode-powertools",
    "displayName": "Power Tools",
    "description": "A set of useful and handy tools for Visual Studio Code.",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": []
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/fs-extra": "^5.0.4",
        "@types/lodash": "^4.14.119",
        "@types/mocha": "^2.2.42",
        "@types/node": "^8.10.25",
        "@types/p-queue": "^2.3.1",
        "tslint": "^5.8.0",
        "typescript": "^3.2.2",
        "vscode": "^1.1.25"
    },
    "author": "e.GO Digital GmbH, Aachen, Germany",
    "license": "LGPL-3.0",
    "homepage": "https://github.com/egodigital/vscode-powertools#readme",
    "keywords": [
        "Tools",
        "eGO",
        "multi-root ready"
    ],
    "readmeFilename": "README.md",
    "icon": "icon.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/egodigital/vscode-powertools.git"
    },
    "dependencies": {
        "fs-extra": "^7.0.1",
        "lodash": "^4.17.11",
        "vscode-helpers": "^2.12.0"
    }
}
